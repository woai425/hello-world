#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_dva-cli@0.7.10@dva-cli/bin/dva" "$@"
  ret=$?
else 
  node  "$basedir/../_dva-cli@0.7.10@dva-cli/bin/dva" "$@"
  ret=$?
fi
exit $ret
